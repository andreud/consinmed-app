<?xml version='1.0' encoding='utf-8'?>
<widget id="io.consinmed.vendedor" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CONSINMED</name>
    <description>
        Aplicaci√≥n para generar pedidos.
    </description>
    <author email="andresxv@gmail.com" href="http://consinmed.com">
       	Andres Hernandez / CONSINMED
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
